var HC_PC_MENU_01 = new Array();
var HC_PC_MENU_02 = new Array();
var HC_PC_MENU_03 = new Array();
var HC_PC_MENU_04 = new Array();
var HC_PC_MENU_05 = new Array();
var HC_PC_MENU_06 = new Array();
var HC_PC_MENU_07 = new Array();
var HC_PC_MENU_08 = new Array();

for (var i =0 ; i < hcPcMenuArr.length; ++i){
    var hcPcMenuObj = hcPcMenuArr[i];
    var gnbynStr    = hcPcMenuObj.gnbyn;
    if(gnbynStr != "y"){
        continue;
    }
    if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="01"){
        HC_PC_MENU_01.push(hcPcMenuObj)
    }
    else if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="02"){
        HC_PC_MENU_02.push(hcPcMenuObj)
    }
    else if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="03"){
        HC_PC_MENU_03.push(hcPcMenuObj)
    }
    else if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="04"){
        HC_PC_MENU_04.push(hcPcMenuObj)
    }
    else if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="05"){
        HC_PC_MENU_05.push(hcPcMenuObj)
    }
    else if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="06"){
        HC_PC_MENU_06.push(hcPcMenuObj)
    }
    else if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="07"){
        HC_PC_MENU_07.push(hcPcMenuObj)
    }
    else if(hcPcMenuObj.psnlWebMenuCd.substr(0,2)=="08"){
        HC_PC_MENU_08.push(hcPcMenuObj)
    }
}
